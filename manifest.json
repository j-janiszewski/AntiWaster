{
    "manifest_version": 3,
    "version": "1.1",
    "name": "AntiWaster",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    }
}